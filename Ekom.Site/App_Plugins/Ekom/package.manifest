            "editor": {
                "valueType": "STRING",
                "view": "~/App_Plugins/Ekom/Datatypes/CouponEditor/ekomCoupon.html"
            }
        },
		{          
            name: "Ekom Country Picker",
			alias: "Ekom.Country",                   
			editor: {
				view: "~/App_Plugins/Ekom/DataTypes/CountryPicker/ekmCountry.html"
